{
  "name": "@k0lyan/nestjs-prisma-graphql-generator",
  "version": "0.4.10",
  "description": "Prisma generator for NestJS 11 GraphQL with optimized query building from GraphQL selections",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nestjs-prisma-graphql-generator": "dist/cli/generator.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/cli/generator.js",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "prisma",
    "prisma-generator",
    "nestjs",
    "graphql",
    "typescript",
    "code-generator",
    "nestjs-graphql",
    "prisma-nestjs",
    "n+1",
    "query-optimization"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": "https://github.com/k0lyan/nestjs-prisma-graphql-generator#readme",
  "bugs": {
    "url": "https://github.com/k0lyan/nestjs-prisma-graphql-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k0lyan/nestjs-prisma-graphql-generator.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@prisma/generator-helper": "^7.0.0",
    "@prisma/internals": "^7.0.0",
    "graphql-parse-resolve-info": "^4.14.0",
    "pluralize": "^8.0.0",
    "ts-morph": "^27.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0",
    "@nestjs/graphql": ">=12.0.0",
    "@prisma/client": ">=7.0.0",
    "graphql": "^16.0.0",
    "graphql-scalars": "^1.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.0"
  }
}
